{
  "apiVersion": 1,
  "info": {
    "ECMAScriptVersion": "ECMAScript 5",
    "name": "Hoc Vien Truyen Tranh",
    "lang": "Vietnamese",
    "origins": [
      "http://hocvientruyentranh.com"
    ],
    "cookies": true
  },
  "categories": [{
    "info": {
      "name": "All Manga",
      "originIndex": "0",
      "path": "/manga/all",
      "type": "plain",
      "params": [{
        "name": "page",
        "options": [],
        "default": "1"
      }]
    },
    "parsers": {
      "_uri": {
        "link": {
          "use": "selector",
          "type": "multiple",
          "config":{
            "selector": "table.table-hover>tbody>tr>td:nth-child(1)>a",
            "attribute": "href"
          }
        }
      },
      "_pagination": {
        "primary": {
          "use": "selector",
          "type": "multiple",
          "config": {
            "selector": ".pagination.no-margin>li>a",
            "attribute": ""
          }
        },
        "link": {
          "use": "selector",
          "type":"multiple",
          "config":{
            "selector": ".pagination.no-margin>li>a",
            "attribute": "href"
          }
        }
      },
      "_prev": {
        "link": {
          "use": "selector",
          "type":"single",
          "config":{
            "selector": ".pagination.no-margin>li>a[rel=prev]",
            "attribute": "href"
          }
        }
      },
      "_next": {
        "link": {
          "use": "selector",
          "type":"single",
          "config":{
            "selector": ".pagination.no-margin>li>a[rel=next]",
            "attribute": "href"
          }
        }
      },
      "title": {
        "primary": {
          "use": "selector",
          "type":"single",
          "config":{
            "selector": "table.table-hover>tbody>tr>td:nth-child(1)>a",
            "attribute": "title"
          }
        }
      },
      "cover": {
        "link":{
          "use": "selector",
          "type":"single",
          "config": {
            "selector": "table.table-hover>tbody>tr>td:nth-child(1)>a",
            "attribute": "data-thumbnail"
          }
        }
      }
    }
  }]
}